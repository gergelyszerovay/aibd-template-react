{
  "name": "@gergelyszerovay/aibd-template-react",
  "version": "1.0.0",
  "description": "A React boilerplate template with Vite, TailwindCSS, and Zustand for AIBD development projects",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "pnpm exec vite --host=127.0.0.1 --port=4300",
    "build": "pnpm exec tsc -b && vite build",
    "lint": "pnpm exec eslint .",
    "preview": "pnpm exec vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@rx-angular/import-graph-visualizer": "^0.2.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^18.3.6",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "3.1.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.24.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "jsdom": "^26.0.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.0",
    "vite": "^5.4.17",
    "vitest": "^3.1.1"
  },
  "packageManager": "pnpm@10.7.1"
}